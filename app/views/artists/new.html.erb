<h1>New Artist Form</h1>
<%= form_for @artist do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br><br>
  <%= f.label :bio %><br>
  <%= f.text_area :bio, :rows => 5, :cols => 100 %><br><br>
  <%= f.submit %>
<% end %>
<button type="button" class="btn-outline-primary">
  <%= link_to 'Back', artists_path %>
</button>
