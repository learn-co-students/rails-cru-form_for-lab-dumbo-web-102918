<h1>Add New Song</h1>

<%= form_for @song do |f| %>
  <%= f.label :Title %><br>
  <%= f.text_field :name %><br><br>

  <%= f.label :Artist %><br>
  <%= f.text_field :artist_id %><br><br>

  <%= f.label :Genre %><br>
  <%= f.text_field :genre_id %><br><br>
  <%= f.submit %>
<% end %>
<button type="button" class="btn-outline-primary">
  <%= link_to 'Back', songs_path %>
</button>
