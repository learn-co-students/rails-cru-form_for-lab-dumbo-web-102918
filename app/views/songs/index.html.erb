<h1>All Songs</h1>
<table class="table table-bordered">
  <tr>
    <th>Title</th>
    <th>Artist</th>
    <th>Genre</th>
  </tr>
  <%@songs.each do |song| %>

  <tr>
    <td><p><%= link_to song.name, song_path(song)%></p></td>
    <td><%=link_to song.artist.name, artist_path(song.artist)%></td>
    <td><%=link_to song.genre.name, genre_path(song.genre)%></td>
  </tr>
<% end %>
</table>
<button type="button" class="btn-outline-primary">
  <%= link_to 'Add New Song', new_song_path %>
</button>
